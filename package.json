{
  "name": "mlir-inc-previewer",
  "icon": "icon.png",
  "displayName": "MLIR Inc Previewer",
  "description": "Open MLIR .inc files side by side",
  "version": "0.0.1",
  "publisher": "yangjianchao16",
  "repository": {
    "type": "git",
    "url": "https://github.com/ConvolutedDog/mlir-inc-previewer.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:cpp",
    "onLanguage:c"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mlir-inc-previewer.openSide",
        "title": "MLIR Inc: Expand/Collapse Preview"
      },
      {
        "command": "mlir-inc-previewer.cleanAll",
        "title": "MLIR Inc: Clean All Preview Content"
      },
      {
        "command": "mlir-inc-previewer.navigateNext",
        "title": "MLIR Inc: Navigate Next Preview"
      },
      {
        "command": "mlir-inc-previewer.showHelp",
        "title": "MLIR Inc: Show Help"
      }
    ],
    "keybindings": [
      {
        "command": "mlir-inc-previewer.openSide",
        "key": "ctrl+shift+u",
        "mac": "ctrl+shift+u",
        "when": "editorTextFocus && editorLangId =~ /cpp|c/"
      },
      {
        "command": "mlir-inc-previewer.cleanAndSave",
        "key": "ctrl+s",
        "mac": "cmd+s",
        "when": "editorTextFocus && editorLangId =~ /cpp|c/"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "mlir-inc-previewer.openSide",
          "group": "mlir_group@1",
          "when": "editorTextFocus && editorLangId =~ /cpp|c/"
        },
        {
          "command": "mlir-inc-previewer.cleanAll",
          "group": "mlir_group@2",
          "when": "editorTextFocus && editorLangId =~ /cpp|c/"
        },
        {
          "command": "mlir-inc-previewer.navigateNext",
          "group": "mlir_group@3",
          "when": "editorTextFocus && editorLangId =~ /cpp|c/"
        },
        {
          "command": "mlir-inc-previewer.showHelp",
          "group": "mlir_group@4",
          "when": "editorTextFocus && editorLangId =~ /cpp|c/"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "18.x",
    "typescript": "^5.0.0"
  }
}